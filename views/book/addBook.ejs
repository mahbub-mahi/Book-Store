<div class="books">
  <h3>Add Story</h3>
  <div class="row">
    <form action="/main/" method="POST" class="col s12">
      <div class="row">
        <div class="input-field">
          <label for="title">Book Name</label>
          <input type="text" id="title" name="title" />
        </div>
      </div>
      <div class="row">
        <div class="input-field">
          <label for="title">Author</label>
          <input type="text" id="author" name="author" />
        </div>
      </div>
      <div class="row">
        <div class="btns">
          <input type="submit" value="Save" class="btn main_btn" />
          <a href="/main" class="btn main_btn">Cancel</a>
        </div>
      </div>
    </form>
  </div>
</div>
