<div class="books">
  <h3>Edit Story</h3>
  <div class="row">
    <form action="/editBook/<%= id%>" method="POST" class="col s12">
      <div class="row">
        <div class="input-field">
          <label for="title">Book Name</label>
          <input type="text" id="title" name="title" value="<%= title %>" />
        </div>
      </div>
      <div class="row">
        <div class="input-field">
          <label for="title">Author</label>
          <input type="text" id="author" name="author" value="<%= author %>" />
        </div>
      </div>
      <div class="row">
        <div class="btns">
          <input type="submit" value="Update" class="btn main_btn" />
          <a href="/main" class="btn main_btn">Cancel</a>
        </div>
      </div>
    </form>
  </div>
</div>
